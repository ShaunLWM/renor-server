{
	"name": "genor-server",
	"version": "1.0.0",
	"main": "index.js",
	"repository": "git@github.com:ShaunLWM/genor-server.git",
	"author": "ShaunLWM",
	"license": "MIT",
	"scripts": {
		"start": "yarn serve",
		"build": "yarn build-ts && yarn lint",
		"serve": "node dist/server.js",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"debug": "yarn build && yarn watch-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
		"localdb": "c: && tools/mongodb/current/bin/mongod --dbpath tools/mongodb/renor"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"dayjs": "^1.8.28",
		"dotenv": "^8.2.0",
		"execa": "^4.0.2",
		"express": "^4.17.3",
		"file-type": "^14.6.2",
		"fs-extra": "^9.0.1",
		"helmet": "^3.23.1",
		"image-size": "^0.8.3",
		"imagemin-gifsicle": "ShaunLWM/imagemin-gifsicle",
		"mongoose": "^5.9.20",
		"multer": "^1.4.2",
		"nanoid": "^3.1.10",
		"randomcolor": "^0.5.4",
		"slugify": "^1.4.2"
	},
	"devDependencies": {
		"@types/body-parser": "^1.19.0",
		"@types/cors": "^2.8.6",
		"@types/dotenv": "^8.2.0",
		"@types/download": "^6.2.4",
		"@types/express": "^4.17.6",
		"@types/fs-extra": "^9.0.1",
		"@types/helmet": "^0.0.47",
		"@types/imagemin-gifsicle": "^5.2.0",
		"@types/mongoose": "^5.7.27",
		"@types/multer": "^1.4.3",
		"@types/nanoid": "^2.1.0",
		"@types/randomcolor": "^0.5.5",
		"concurrently": "^5.2.0",
		"download": "^8.0.0",
		"node-fetch": "^2.6.1",
		"poster": "^0.0.9"
	}
}
